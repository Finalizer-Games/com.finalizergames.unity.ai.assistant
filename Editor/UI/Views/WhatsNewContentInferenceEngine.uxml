<ui:UXML xmlns:ui="UnityEngine.UIElements"
         xmlns:chat="Unity.AI.Assistant.Editor.SessionBanner"
         xmlns="UnityEngine.UIElements" editor-extension-mode="False">
    <ui:VisualElement class="mui-whats-new-page-root">
        <ui:VisualElement name="page1" class="mui-whats-new-page-section">
            <ui:Label text="Meet Inference Engine" class="mui-whats-new-page-header"/>
            <ui:Label text="Inference Engine enables new player experiences where real-time (high FPS) inference results are desired, such as with real-world interactions like motion or speech detection, animation effects, and game agents."
